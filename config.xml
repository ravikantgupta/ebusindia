<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ebusindia.bus" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>ebusindia</name>
    <description>
        Online volvo bus booking | AC Volvo Bus Booking Online Services
    </description>
    <author email="info@ebusindia.in" href="https://www.ebusindia.in">
       Webidea team
    </author>
    <content src="index.html" />
   <plugin name="cordova-plugin-device" spec="^1.1.0" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="com.telerik.plugins.nativepagetransitions" spec="^0.6.5" />
    <plugin name="cordova-plugin-inappbrowser" spec="^2.0.2" />
    <plugin name="cordova-plugin-datepicker" spec="^0.9.3" />
     <plugin name="cordova-plugin-whitelist" version="1"/>
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
   
	<plugin name="phonegap-plugin-push" spec="2.0.0"/>
    
	
<!--<plugin name="cordova-plugin-sim" source="npm"/>-->
	<platform name="android">
         <resource-file src="google-services.json" target="app/google-services.json" />   

   </platform>
    
   
<preference name='phonegap-version' value='cli-7.1.0' />
	<preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
